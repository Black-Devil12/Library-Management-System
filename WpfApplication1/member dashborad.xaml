            <Button x:Name="btn_book_member" Content="Books" HorizontalAlignment="Left" Margin="0,82,0,0" VerticalAlignment="Top" Width="300" Background="#FF013888" Foreground="White" BorderBrush="#FF013888" FontFamily="Arial " Cursor="Hand" Click="btn_book_member_Click" FontSize="26.667"/>
            <Button x:Name="btn_logout" Content="Logout" HorizontalAlignment="Left" Margin="0,140,0,0" VerticalAlignment="Top" Width="300" Background="#FF013888" Foreground="White" BorderBrush="#FF013888" FontFamily="Arial " Cursor="Hand" Click="btn_logout_Click" FontSize="26.667"/>
            <DataGrid x:Name="dg_book_member" HorizontalAlignment="Left" Margin="305,27,0,0" VerticalAlignment="Top" Height="658" Width="673"/>

        </Grid>

    </Grid>
</Window>
