<Window x:Class="WpfApplication1.member_dashborad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="member_dashborad" Height="750" Width="1000">
    <Grid Margin="0,0,-6.4,-19.6">
        <Grid Margin="0,0,10,10">
            <Grid x:Name="Dashborad1" HorizontalAlignment="Left" Height="719" VerticalAlignment="Top" Width="300" Background="#FF013888"/>
